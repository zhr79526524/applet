page{
	background-color: #F4F4F4;
}
.container{
	margin-top: 50rpx;
}
.form{
}
form .form_acito view{
	display: flex;
	background-color: white;
	margin-top: 7rpx;
	font-size: 34rpx;
	padding: 20rpx;
}
.form_acito text{
	margin-left: 20rpx;
}
form input{
	height: 100%;
	margin-left: 15%;
	font-size: 28rpx;
}
.forget{
	color: #fb7299;
	font-size: 28rpx;
	text-align: right;
	display: block;
	padding: 15rpx;
	font-weight: 500;
}
.click{
	display: flex;
	justify-content: space-between;
}
.click button{
	width: 46%;
	border-radius: 5%;
	border:solid 1px  #fb7299;
	margin-top: 50rpx;
	color: #fb7299;
	outline: none;
}
.regiter{
	
}
.login{
}
.style{
	color: white !important;
	background-color: #fb7299;
}
.user_manual{
	display: flex;
	margin-top:50rpx;
	justify-content: center;
	font-size: 28rpx;
	color: #666;
}
.user_manual navigator text{
	color: #fb7299;
	display: inline-block;
}
.footer{
	width: 100%;
	position: absolute;
	bottom: 20%;
	padding: 5%;
}
.footer_son{
	width: 100%;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.borer_left{
	width: 30%;
	height: 2rpx;
	background: #ccc;
}
.borer_text{
	width: 30%;
	text-align: center;
	font-size: 26rpx;
}
.borer_right{
	width: 30%;
	height: 2rpx;
	background: #ccc;
}
.href{
	display: flex;
	justify-content: center;
	margin-top: 50rpx;
}
.href view{
	margin: 50rpx;
}
.href text{
	display: inline-block;
	margin-top: 10rpx;
	color: #666;
	font-size: 26rpx;
}
.href_left,.href_right{
	display: flex;
	flex-direction: column;
	align-items: center;
}
.href image{
	width: 50rpx;
	height: 50rpx;
}